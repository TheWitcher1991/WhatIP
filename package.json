{
  "name": "whatip",
  "version": "1.0.0",
  "main": "main.js",
  "bin": "cli.js",
  "private": true,
  "scripts": {
    "start": "electron .",
    "start-all": "node server && electron .",
    "dev": "electron . --debug",
    "package": "npm-run-all package:*",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --out=build --icon=",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=build"
  },
  "author": "GitHub: TheWitcher1991",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheWitcher1991/whatip"
  },
  "devDependencies": {
    "asar": "^3.0.3",
    "devtron": "^1.4.0",
    "electron": "^7.1.7",
    "electron-log": "^4.1.1",
    "electron-packager": "^14.1.1",
    "electron-winstaller": "^4.0.0"
  },
  "engines": {
    "node": ">= 12.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.x"
  }
}
